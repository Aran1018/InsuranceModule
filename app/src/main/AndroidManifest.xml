<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.org.prism.insurancemodule">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MyInsuranceActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AskPriseActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".activity.LargePictureActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".activity.MyReportActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".activity.PersonalDataActivity"
            android:launchMode="singleTask" />
        <activity android:name=".activity.EditPersonalDataActivity" />
        <activity android:name=".activity.IDCardSampleFrontActivity" />
        <activity android:name=".activity.IDCardSampleOppositeActivity"></activity>
    </application>

</manifest>